<!doctype html>
<html>

<head>
  <title>LinkUp</title>
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
  <link rel="stylesheet" href="buttonstyles.css">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <link rel="stylesheet" href="assets/css/main.css" />
  <noscript>
    <link rel="stylesheet" href="assets/css/noscript.css" />
  </noscript>
  <script src="http://code.jquery.com/jquery-1.10.2.js"></script>
  <script src="http://code.jquery.com/ui/1.10.4/jquery-ui.js"></script>
  <style>
    body {
      padding-top: 80px;
      word-wrap: break-word;
    }

    .well {
      position: relative;
      text-align: center;
      margin: 0 auto;
    }
  </style>
</head>

<body>
  <div class="container">

    <div class="page-header text-center">
      <div class="main-wrapper">
        <!-- <i class="material-icons">attach_file</i>
          <i class="material-icons">settings</i> -->
        <!-- <i class="material-icons">refresh</i> -->

        <a id="findFriends" href="#friends"><i class="material-icons">search</i></a>
        <a href="/navigation"><i class="material-icons">explore</i></a>
        <a href="#friend-requests"></a>  <i class="material-icons">add</i>
        <h2>Your Profile</h2>
        <img class="img-circle" src="<%=user.facebook.picture %>" width="50" height="50" />
        <br>
        <%= user.facebook.name %>
          <br>
          <!-- <i class="material-icons">autorenew</i> -->
      </div id="sign-out-color">
      <a href="/logout" class="fa fa-sign-out">Logout</a>
    </div>


  </div>
  <!-- Wrapper-->
  <div id="wrapper">

    <!-- Nav -->
    <nav id="nav">
      <!-- <a href="#me" class="icon fa-home active">
        <span>Home</span> -->
      </a>
      <a href="#friends" class="fa fa-users">
        <span>Friends</span>
      </a>
      <a href="#message" class="fa fa-commenting">
        <span>Messages</span>
      </a>
      </a>
    </nav>

    <!-- Main -->
    <div id="main">

      <!-- Me -->
      <article id="me" class="panel">
        <header>
        </header>
        <a href="#friends" class="img-circle">
          <span class="arrow icon fa-chevron-right">
          </span>
          <img class="img-circle" src="<%=user.facebook.picture %>" width="50" height="50" />
        </a>
      </article>
      <div>
        <article id="friends" class="panel">
          <header>
            <h2>Your Friends</h2>
          </header>
          <p>
            Tap one of your friends to send them a request!
          </p>
          <section>
            <div class="row">
              <!--friends list starts here-->
              
            </div>
          </section>
        </article>
      </div>
       <div>
       Message
       <article id="message" class="panel">
          <header>
            <h2>Send a Message</h2>
          </header>
          <form action="#" method="post">
            <div>
              <div class="row">
                <div class="6u 12u$(mobile)">
                  <input id='name-field' type="text" name="name" placeholder="Name" />
                </div>

                <div class="12u$">
                  <textarea name="message" placeholder="type your message..." rows="4"></textarea>
                </div>
                <div class="12u$">
                  <input type="submit" value="Send" />
                </div>
              </div>
            </div>
          </form>
        </article>
       </div>

      <script>
        $(function() {
          var myList = [
            "Addison", "Ben", "Christine", "Chloe", "Cris", "Dane", "David H", "David Z", "Emily", "Imtaek", "Jack", "Jesse Z", "Jonathan", "Pericles", "Peter", "Philippe", "Sergio", "Simon", "Trevor"
          ];
          $("#myval").autocomplete({
            source: myList
          });
        });


      </script>

      <script src="assets/js/jquery.min.js"></script>
      <script src="assets/js/skel.min.js"></script>
      <script src="assets/js/skel-viewport.min.js"></script>
      <script src="assets/js/util.js"></script>
      <!--[if lte IE 8]><script src="assets/js/ie/respond.min.js"></script><![endif]-->
      <script src="assets/js/main.js"></script>
      <script src="scripts.js"></script>
</body>

</html>
